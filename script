node ('master'){
    try{
        echo "build"
    currentBuild.result = 'SUCCESS'
    }    
    catch (err) {
        currentBuild.result = 'FAILURE'
    }
    finally {
        mail to: 'shabbir0473@gmail.com',
        subject: "status of pipeline",
        body: "${env.BUILD_URL} has result ${currentBuild.result}",
    }
}
